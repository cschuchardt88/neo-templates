{
  "$schema": "http://json.schemastore.org/template",
  "name": "Neo Smart Contract Project (Basic)",
  "shortName": "neosmartcontract",
  "sourceName": "NeoSmartContractProject",
  "description": "Smart contract for neo n3 blockchain.",
  "author": "Christopher Schuchardt",
  "classifications": [ "neo", "smart contract", "basic" ],
  "identity": "Neo.Events.Templates",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "symbols": {
    "NeoVersion": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "3.6.0"
        }
      ],
      "defaultValue": "3.6.0",
      "replaces": "TemplateNeoVersion"
    },
    "ContractName": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "Contract1",
      "replaces": "TemplateContractName",
      "fileRename": "TemplateContractName"
    },
    "ContractAuthor": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "<Your Name Or Company Here>",
      "replaces": "TemplateContractAuthor"
    },
    "ContractDescription": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "<Description Here>",
      "replaces": "TemplateContractDescription"
    },
    "ContractEmail": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "<Your Public Email Here>",
      "replaces": "TemplateContractEmail"
    }
  }
}
